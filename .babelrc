{
    "presets": [[    
        "env", {
            "targets": { "browsers": ["last 2 versions", "safari >= 7"] 
        }
    }], "stage-0", "react" ],
    "plugins": [
        [
            "transform-object-rest-spread", { "useBuiltIns": true },
            "transform-runtime" 
        ]
    ],
    "env": {
        "production": {
            "plugins": [
                "transform-react-constant-elements", 
                "transform-react-inline-elements"
            ]
        }
    }
}
